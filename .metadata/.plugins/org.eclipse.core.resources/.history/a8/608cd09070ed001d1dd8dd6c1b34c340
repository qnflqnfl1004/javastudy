package study;

public class lamda {

  public static void main(String[] args) {

    Lamda01Function f = () -> System.out.println("람다01테스트 출력");
    
    f.test();
    
  }

}

@FunctionalInterface    // 단 하나의 추상메서드를 갖고 있는 인터페이스.  올바르게 함수형 인터페이스를 정의하였는지 체크해준다.
interface Lamda01Function{
    public void test();
}
